{
  "name": "language-markdown",
  "version": "0.3.1",
  "description": "Adds support for Markdown (including Github flavored, Markdown Extra, CriticMark, and YAML/TOML front-matter) based on CommonMark and common sense.",
  "keywords": [
    "markdown",
    "commonmark",
    "gfm",
    "github flavored",
    "markdown extra",
    "criticmark",
    "front-matter"
  ],
  "repository": "https://github.com/burodepeper/language-markdown",
  "bugs": {
    "url": "https://github.com/burodepeper/language-markdown/issues"
  },
  "main": "lib/main.coffee",
  "license": "MIT",
  "private": true,
  "engines": {
    "atom": "*"
  },
  "dependencies": {
    "lodash": "^3.10.1",
    "lib-ass": "0.0.6",
    "season": "^5.3"
  },
  "coffeelintConfig": {
    "max_line_length": {
      "level": "ignore",
      "value": 80
    }
  }
}
