key: 'github'
patterns: [
  {
    match: '(?<=^|\\s|"|\'|\\(|\\[)((@)(\\w[-\\w:]*))(?=[\\s"\'.,;\\)\\]])'
    captures:
      1: name: 'username.reference.gfm.variable.md'
      2: name: 'punctuation.md'
  }
  {
    match: '(?<=^|\\s|"|\'|\\(|\\[)((#)(\\d+))(?=[\\s"\'\\.,;\\)\\]])'
    captures:
      1: name: 'issue.reference.gfm.variable.md'
      2: name: 'punctuation.md'
  }
]
