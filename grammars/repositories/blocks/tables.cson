key: 'tables'
patterns: [
  name: 'table.storage.md'
  begin: '^\\|'
  beginCaptures:
    0: name: 'punctuation.md'
  end: '(\\|)?\\s*$'
  endCaptures:
    1: name: 'punctuation.md'
  patterns: [
    {
      match: '(?<= )\\|(?= )'
      name: 'punctuation.md'
    }
    {
      match: '(:?)(-+)(:?)'
      captures:
        1: name: 'alignment.punctuation.md'
        2: name: 'punctuation.md'
        3: name: 'alignment.punctuation.md'
    }
    {
      include: '#inlines'
    }
  ]
]
