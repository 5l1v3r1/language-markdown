# http://spec.commonmark.org/0.22/#example-307
"`<a href="`">`" {
  text.md {
    code.md {
      '`': punctuation.md
      '<a href="'
      '`': punctuation.md
    }
    '">`'
  }
}

# http://spec.commonmark.org/0.22/#example-308
'<a href="`">`' {
  text.md {
    html.md {
      '<': punctuation.md
      'a': tag.md
      ' '
      attribute.md {
        'href': name.md
        '=': punctuation.md
        value.md {
          '"': punctuation.md
          '`'
          '"': punctuation.md
        }
      }
      '>': punctuation.md
    }
    '`'
  }
}

