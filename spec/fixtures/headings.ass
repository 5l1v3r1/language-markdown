# http://spec.commonmark.org/0.22/#example-32
'#                  foo' {
  text.md {
    heading.md {
      heading-1.md {
        '#': punctuation.md
        '                  '
        'foo'
      }
    }
  }
}

# http://spec.commonmark.org/0.22/#example-33
'  ## Heading' {
  text.md {
    heading.md {
      heading-2.md {
        '  '
        '##': punctuation.md
        ' '
        'Heading'
      }
    }
  }
}

